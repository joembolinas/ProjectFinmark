{
  "server": {
    "port": 3000,
    "host": "localhost"
  },
  "loadBalancer": {
    "servers": [
      "http://localhost:3001",
      "http://localhost:3002",
      "http://localhost:3003"
    ],
    "algorithm": "round-robin"
  },
  "monitoring": {
    "interval": 5000,
    "logLevel": "info",
    "enableWebSocket": true
  },
  "security": {
    "enableFirewall": true,
    "enableVPN": false,
    "maxConnections": 1000
  }
}
